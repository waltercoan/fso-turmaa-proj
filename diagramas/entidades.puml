@startuml
skinparam classAttributeIconSize 0
caption Diagrama de entidades
namespace entity {
    class Cliente {
        - long id
        - String nome
        - String endereco

        + getId():long
        + setId(id:long):void
        + getNome():String
        + setNome(nome:String):void
        + getEndereco():String
        + setEndereco(endereco:String ):void
        
    }
     class Cidade {
        - long id
        - String nome
        - String estado

        + getId():long
        + setId(id:long):void
        + getNome():String
        + setNome(nome:String):void
        + getEstado():String
        + setEstado(estado:String ):void
        
    }

    Cliente "*" -> "1" Cidade
}
@enduml